.padding-top90{
    padding-top:90px;
    @media(max-width:991px){
        padding-top:50px;
    }
}
.testimonial_1,
.testimonial_2,
.testimonial_3{
	.template-header{
	    text-align: center;
	    line-height: 1;
	    &-logo{
	        width:60px;
	        height:60px;
	        margin:0 auto;
	        img{
	            width:100%;
	        }
    }
    &-title{
        font-family: 'SourceSansPro-Regular';
        font-size:16px;
        color:#9e9e9e;
        margin-top:35px;
    }
    &-style{
        font-family: 'Raleway-Regular';
        font-size:30px;
        color:#e1b55a;
        margin-top:15px;
    }
}
	color:black;
	input,textarea,button{
		border-radius:0;
	}
	font-family: 'SourceSansPro-Regular';
	font-size:16px;
	.testimonial-header{
		text-align: center;
		.title-1{
			color:#9e9e9e;
			margin-top:35px;
		}
		.title-2{
			font-family: 'Raleway-Regular';
			font-size:30px;
			color:#e1b55a;
			margin-top:5px;
		}
	}
	.testimonial-item-quote{
		p{
			line-height: 30px; 
		}
	}
	//custom dot owl carousel
	.owl-controls .owl-pagination .owl-page span{
		width:30px;
		border-top:2px solid #dddddd;
		height:0;
		transition:all 0.2s;
		-webkit-transition:all 0.2s;
		-moz-transition:all 0.2s;
		-o-transition:all 0.2s;
	}
	.owl-controls .owl-pagination .owl-page.active span,
	.owl-controls .owl-pagination .owl-page:hover span{
		border-top:4px solid #e2b75c;
	}
	.owl-controls .owl-pagination{
		margin-top:50px;
	}
}
.testimonial_1{
	background:url(../../images/blocks/testimonial/testimonial-bg1.png) no-repeat;
	background-size:cover;
	color:white;
	padding-bottom:145px;
	@media(min-width:992px){
		.owl-controls .owl-pagination{
			display:none;
		}
	}
	.testimonial-content{
		margin-top:105px;
		@media(max-width:992px){
			padding:0 15px;
			margin-top:50px;
		}
	}
	.carousel-prev,
	.carousel-next{
		width:40px;
		height:40px;
		position:absolute;
		border:1px solid #fff;
		font-size:30px;
		border-radius:50%;
		text-align: center;
		top:50%;
	    cursor: Pointer;
    	z-index: 9;
    	@media(max-width:991px){
    		display:none;
    	}
		.fa{
		    position: absolute;
		    top: 2px;
		}
	}
	.carousel-prev{
		left:50%;
		transform:translateX(-500px) translateY(-50%);
		-webkit-transform:translateX(-500px) translateY(-50%);
		-moz-transform:translateX(-500px) translateY(-50%);
		-o-transform:translateX(-500px) translateY(-50%);
		.fa{
		    left: 11px;
		}
		@media(max-width:992px){
		    bottom: -50px;
		    top:initial;
		    left: 0;
		    transform: initial;
		}
	}
	.carousel-next{
		right:50%;
		transform:translateX(500px) translateY(-50%);
		-webkit-transform:translateX(500px) translateY(-50%);
		-moz-transform:translateX(500px) translateY(-50%);
		-o-transform:translateX(500px) translateY(-50%);
		.fa{
		    right: 11px;
		}
		@media(max-width:992px){
		    bottom: -50px;
		    top:initial;
		    right: 0;
		    transform: initial;
		}
	}
	.testimonial-item{
		text-align: center;
		&-img{
			width:120px;
			height:120px;
			border-radius:50%;
			-moz-border-radius:50%;
			margin:0 auto;
			border:4px solid #fff;
			img{
				width:100%;
			}
		}
		&-quote{
			max-width:600px;
			white-space: pre-line;
			margin:0 auto;
			margin-top:20px;
			p{
				margin-bottom:0;
			}
		}
		&-name{
			font-size:24px;
		}
		&-position{
			color:#e1b55a;
		}
	}
}
.testimonial_1.bg-ttn-cutome10{
	background-image:url(../../images/blocks/testimonial/bg_custome_10_3.jpg);
	background-repeat:no-repeat;
}
.testimonial_2{
	background:url(../../images/blocks/testimonial/testimonial-bg2.png) no-repeat;
	background-size:cover;
	padding-bottom:100px;
	.testimonial-content{
		margin-top:75px;
		@media(max-width:991px){
			margin-top:50px;
		}
	}
	.testimonial-item{
		background:white;
		position:relative;
		padding:15px 30px 40px 35px;
		margin-top:60px;
		box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.25);
		-webkit-box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.25);
		-moz-box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.25);
		margin-bottom:3px;
		&-img{
			position:absolute;
			width:120px;
			height:120px;
			border-radius:50%;
			border:5px solid white;
			box-shadow: 0px 3px 5px 0px rgba(0, 0, 0, 0.2); 
			top:-60px;
			left:40px;
			img{
				width:100%;
			}
			@media(max-width:767px){
				left:50%;
				transform:translateX(-50%);
				-webkit-transform:translateX(-50%);
				-moz-transform:translateX(-50%);
				-o-transform:translateX(-50%);
			}
		}
		&-name{
			padding-left:155px;
			font-size:24px;
			color:#303030;
			@media(max-width:767px){
				padding-left:0;
				padding-top:70px;
				text-align: center;
			}
		}
		&-position{
			padding-left:155px;
			color:#e1b55a;
			@media(max-width:767px){
				padding-left:0;
				text-align: center;
			}
		}
		&-quote{
			margin-top:35px;
			@media(max-width:767px){
				text-align: center;
			}
		}
	}
	.owl-wrapper .owl-item{
		padding: 0px 15px;
	}
}
