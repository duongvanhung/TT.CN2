	.padding-top90{
    padding-top:90px;
    @media(max-width:991px){
        padding-top:50px;
    }
}
.footer_1,
.footer_2,
.footer_3{
	font-family: 'SourceSansPro-Regular';
	font-size:16px;
	color:white;
	.icons-social{
		a{
			width:30px;
			display:inline-block;
			img{
				width:100%;
			}
		}
	}
	input,button,textarea{
		border-radius:0;
	}

}
.footer_1{
	background:url(../../images/blocks/footer/background1.png) no-repeat;
	background-size:cover;
	padding-bottom:30px;
	p{
		margin-bottom:0;
	}
	.logo{
		margin:0 auto;
		width:60px;
		img{
			width:100%;
		}
	}
	.title{
		margin-top:45px;
		p{
			text-align:center;
		}
		p:nth-child(1){
			font-family: 'Raleway-Bold';
			font-size:48px;
			text-transform: uppercase;
			line-height: 1;
		}
		p:nth-child(2){
			font-family: 'SourceSansPro-Regular';
			font-size:24px;
			color:#e1b55a;
			margin-top:5px;
		}
		p:nth-child(3){
		    white-space: pre-line;
		    max-width:615px;
		    margin:0 auto;
		    margin-top:15px;
		}
	}
	.icons-social{
		margin-top:30px;
		text-align:center;
	}
	.coppy-right{
		margin-top:55px;
		text-align: center;
	}
}

.footer_2{
	background:#2a2a2a;
	padding-top:35px;
	.footer-title{
		font-family: 'SourceSansPro-Bold';
		margin-top:40px;
		@media(max-width:767px){
			text-align: center;
		}
	}
	.footer-content{
		line-height: 25px;
	    margin-top: 25px;
	    @media(max-width:767px){
	    	text-align:center;
	    }
	}
	.footer-subscrible{
		background:#323232;
		padding:40px 35px 40px 35px;
		@media(max-width:991px){
			margin-top:15px;
			padding:40px 15px 40px 15px;
		}
		.footer-title{
			margin-top:0;
		}
		.footer-content{
			margin-top:10px;
		}
	}
	.icons-social{
		margin-top:30px;
		@media(max-width:767px){
			text-align: center;
		}
	}
	.footer-group-subscrible{
		display: flex;
		margin-top:35px;
		// @media(max-width:767px){
		// 	display:block;
		// }
		input{
			font-family:FontAwesome, "SourceSansPro-Regular";
			font-style:normal;
			font-weight: normal;
			background:transparent;
			border:1px solid #d8ae57;
			height:50px;
			width:100%;
			outline: none;
			padding-left:25px;
			&:-ms-input-placeholder{
				color:white;
			}
			&::-moz-placeholder{
				color:white;
			}
			&:-moz-placeholder {
				color:white;
			}
			&::-webkit-input-placeholder{
				color:white;
			}
		}
		button{
			font-family: 'SourceSansPro-Bold';
			width:170px;
			height:50px;
			background:#d8ae57;
			border-style:none;
			outline: none;
		    flex-shrink: 0;
		    text-align: center;
		    @media(max-width:767px){
		    	width:100px;
		    	// margin-top:15px;
		    }
		    span{
		    	display: block;
		    	margin:0 auto;
		    	text-align: center;
		    }
		}
	}
	.coppy-right{
		border-style:solid;
		border-width:1px 0px 0px 0px;
		border-color:#404040;
		margin-top:50px;
		padding-top:20px;
		padding-bottom:25px;
		@media(max-width:767px){
			text-align: center;
		}
	}
}
.footer_3{
	background:url(../../images/blocks/footer/background3-1.png) no-repeat;
	background-size:cover;
	padding-top:80px;
	.logo{
		text-align: center;
	}
	.footer-content{
		max-width:685px;
		margin:0 auto;
		white-space: pre-line;
		text-align: center;
		margin-top:30px;
	}
	.footer-app{
		margin-top:55px;
		text-align: center;
		a{
			width:220px;
			height:70px;
			display: inline-block;
			background:white;
			position:relative;
			padding-left:80px;
			padding-top:14px;
			text-decoration:none;
			margin-left:7.5px;
			margin-right:7.5px;
			margin-top:15px;
			text-align: left;
			&:nth-child(2){
				width:240px;
				@media(max-width:991px){
					width:220px;
				}
			}
			img{
				position:absolute;
				top: 13px;
    			left: 15px;
			}
			span{
				display:block;
				&:nth-child(1){
					font-family: 'Raleway-Bold';
					font-size:12px;
					color:#abafb8;
					text-transform: uppercase;
				}
				&:nth-child(2){
					font-family: 'Raleway-Regular';
					font-size:18px;
					color:black;
				}
			}
		}
	}
	.coppy-right{
		border-style:solid;
		border-width:1px 0px 0px 0px;
		border-color:rgba(255,255,255,0.1);
		margin-top:80px;
		padding-top:20px;
		padding-bottom:25px;
		@media(max-width:767px){
			text-align: center;
		}
	}
	.icons-social{
		float:right;
		@media(max-width:767px){
			float:none;
			text-align: cetner;
			margin-top:10px;
		}
	}
}