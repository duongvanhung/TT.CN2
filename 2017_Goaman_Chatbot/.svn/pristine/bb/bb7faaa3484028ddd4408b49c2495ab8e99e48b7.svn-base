	-o-animation: effect-zoom .9s ease-in-out .8s infinite;
	animation: effect-zoom .9s ease-in-out .8s infinite;
}
@keyframes effect-zoom {
	0%{
		transform:translateX(-50%) translateY(-50%) scale(1);
		opacity:0;
	}
    50%{
		transform:translateX(-50%) translateY(-50%) scale(1.2);
		opacity:1;
	}
	100%{
		transform:translateX(-50%) translateY(-50%) scale(1.6);
		opacity:.2;
	}
}
@-wbkit-keyframes effect-zoom {
	0%{
		transform:translateX(-50%) translateY(-50%) scale(1);
		opacity:0;
	}
    50%{
		transform:translateX(-50%) translateY(-50%) scale(1.2);
		opacity:1;
	}
	100%{
		transform:translateX(-50%) translateY(-50%) scale(1.6);
		opacity:.2;
	}
}
@-moz-keyframes effect-zoom {
	0%{
		transform:translateX(-50%) translateY(-50%) scale(1);
		opacity:0;
	}
    50%{
		transform:translateX(-50%) translateY(-50%) scale(1.2);
		opacity:1;
	}
	100%{
		transform:translateX(-50%) translateY(-50%) scale(1.6);
		opacity:.2;
	}
}

