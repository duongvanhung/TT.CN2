.clearboth{
	height:100px;
	display:block;
	width:100%;
}
.comming_soon_1,
.comming_soon_2,
.comming_soon_3,
.comming_soon_4{
	input,textarea,button{
		border-radius:0;
	}
	color: #ffffff;
	.ibg-bg {
	  background-position-x: 20px !important;
	  position: absolute ;
	  z-index:-1;
	}
	.smalltext{
	    font-size: 16px;
	    font-family: SourceSansPro-Bold ;
	}
	@media(max-width:767px){
		.input__field--kaede.active + .input__label--kaede, .input--filled .input__label--kaede{
		    -webkit-transform: translate3d(72%, 0, 0);
	    	transform: translate3d(72%, 0, 0);
		}
	}
}
.comming_soon_1{
	background:url(../../images/blocks/comming_soon/background-cm1.png) no-repeat;
	background-size:cover;
	padding-top:160px;
	padding-bottom:270px;
	@media(max-width:767px){
		padding-bottom:90px;
		padding-top:50px;
	}
	.logo{
		margin-bottom: 40px; 
	}
	.content,.content-2{
		font-family: Raleway-Regular;
	   p:nth-child(1){
			font-size:30px;
			margin-bottom:30px;
	   }
	   p:nth-child(2){
	   		font-size: 18px;
	   		line-height: 35px;
	   }
	   @media(max-width:768px){
	   		text-align: center;
	   }
	}
	.clockdiv-countdown{
		.row{
			margin:0 -7.5px;
			[class*="col-"]{
				padding:0 7.5px;
				margin-top:25px;
			}
		}
		.square{
			text-align: center;
			border : 1px solid #e1b55a;
		}
	}
	.sign-up{
	  margin-top: 40px;
	}
	.content-2{
		margin-top:60px;
		font-family: SourceSansPro-Regular;
		p:nth-child(1){
			font-size:24px;
			margin-bottom:5px;
	   	}
	   	p:nth-child(2){
	   		font-size:16px;
	   	}
	}
	.smalltext{
	    text-align: center;
	    font-weight: bold;
	    background-color: #e1b55a;
	    width:100% ;
	}
	.square{
		width:100%;
		height:135px;
		text-align: center;
		border : 1px solid #e1b55a;
		span{
			font-family: SourceSansPro-Regular;
			line-height: 135px;
			font-size:60px;
		}
	}
}
.comming_soon_2{
	background:url(../../images/blocks/comming_soon/background-cm2.png) no-repeat;
	background-size:cover;
	padding-top:50px;
	@media(max-width:991px){
		padding-left:15px;
		padding-right:15px;
	    background-position-x: -1013px;
	}
}
.comming_soon_2,.comming_soon_3,.comming_soon_4{
	#logo{
	  height: 60px;
	  width: 260px;
	  margin-left: 50px;
	  @media(max-width:768px){
	  	margin-left:auto;
	  	margin-right:auto;
	  	display:block;
	  }
	}
	.col-center{
		text-align: center;
		width:660px;
		display:block;
		padding-bottom:140px;
		@media(max-width:767px){
			width:100%;
			padding-bottom:50px;
		}
		margin:0 auto;
		.title{
			font-family: Raleway-heavy;
			font-size:72px;
			margin-top:90px;
			@media(max-width:767px){
				font-size: 35px;
				margin-top:50px;
			}
		}
		.content{
			font-family: SourceSansPro-Regular;
			font-size:18px;
			margin-top:25px;
			line-height: 35px;
		}
		.clockdiv-countdown{
			@media(max-width:992px){
				padding-left:15px;
				padding-right:15px;
			}
			>.row >[class*="col-"]{
				margin-top:70px;
				@media(max-width:767px){
					margin-top: 30px;
				}
			}
			.smalltext{
				margin-top:30px;
				@media(max-width:767px){
					margin-top:10px;
				}
			}
			.square{
				width:135px;
				height:135px;
				border:3px solid white;
				border-radius:50%;
				@media(max-width:768px){
					margin-left:auto;
					margin-right:auto;
				}
				span{
					line-height: 135px;
					font-size:48px;
				}
			}
		}
		.sign-up{
			margin-top:95px;
			.input__label.input__label--kaede{
				background:white;
				color:black;
			}
			input.input__field.input__field--kaede{
				background:transparent;
				border-style:solid;
				border-width:1px;
				border-color:white;
			}
		}
		.contact{
			margin-top:70px;
			.fa{
				font-size:18px;
				color:white;
				margin:0 10px;
			}
		}
	}

}
.comming_soon_3{
	display:flex;
	width:100%;
	.col-center{
		padding-left:15px;
		padding-right:15px;
	}
	#logo{
		margin-top:50px;
	}
	.col-center .contact{
		margin-top:80px;
	}
	.col-1{
		width:100%;
		background:black;
	}
	.col-2{
		width:100%;
		background:url(../../images/blocks/comming_soon/commingsoon3.jpg) no-repeat;
		background-size: 100% 100%;
		@media(max-width:992px){
			display:none;
		}
	}
}
.comming_soon_4{
	background:url(../../images/blocks/comming_soon/commingsoon4.jpg) no-repeat;
	background-size:cover;
	padding-bottom:260px;
	padding-top:50px;
	@media(max-width:767px){
		padding-bottom:50px;
	}
	.col-center{
		width:100%;
		padding:0 15px;
		.content{
			width:660px;
			margin:0 auto;
			@media(max-width:768px){
				width:100%;
			}
		}
		.clockdiv-countdown{
			text-align: right;
			@media(max-width:1199px){
				text-align: center;
			}
			.square{
				display:inline-block;
				width:90px;
				margin-top:70px;
				margin-right:90px;
				border-style:none;
				@media(max-width:992px){
					display:block;
					margin-right:auto;
					margin-left:auto;
				}
				@media(max-width:1199px) and (min-width:768px){
					display: inline-block;
					margin-left:30px;
					margin-right:30px;
				}
				span{
					font-family: "SourceSansPro-Bold";
					font-size:90px;
				}
				.smalltext{
					margin-top:8px;
					text-align: center;
				}
			}
		}
		.sign-up{
			margin-left:90px;
			margin-top:115px;
			max-width:640px;
			@media(max-width:1199px){
				margin-left: auto;
				margin-right:auto;
			}
		}
		.contact{
			max-width:640px;
			text-align: center;
			margin-left:90px;
			margin-top:40px;
			@media(max-width:1199px){
				margin-left: auto;
				margin-right:auto;
			}
			a{
				width:40px;
				height:40px;
				border-radius:50%;
				border:1px solid white;
				display:inline-block;
				margin-right:12px;
				&:last-child{
					margin-right:0px;
				}
				.fa{
					text-align: center;
					line-height: 40px;
				}
			}
		}
	}
}