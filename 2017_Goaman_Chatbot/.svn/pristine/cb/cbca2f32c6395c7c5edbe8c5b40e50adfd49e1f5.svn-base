.block-list-op-menu {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #3a3d51;
    color: #fff;
    opacity: 1;
    z-index: 9;
    -webkit-transition: all 0.4s;
    -moz-transition: all 0.4s;
    -ms-transition: all 0.4s;
    transition: all 0.4s;
}

    .block-list-op-menu .bnt-menu-cancal, .block-list-op-menu .bnt-menu-accept {
        position: absolute;
        right: 21px;
        bottom: 1px;
        padding-top: 10px;
        background-color: #fff;
        color: #353748;
        width: 100px;
        height: 40px;
        text-align: center;
        cursor: pointer;
        transition: all 0.4s;
        -webkit-transition: all 0.4s;
        -moz-transition: all 0.4s;
        -ms-transition: all 0.4s;
    }

    .block-list-op-menu .bnt-menu-accept {
        right: 134px;
        background-color: #fff;
    }


    .block-list-op-menu .list-item-menu {
        height: 60px;
        width: calc(100% - 40px);
        margin-bottom: 3px;
        background-color: #282937;
        position: absolute;
        left: 20px;
        top: 40%;
        padding: 5px 10px;
        opacity: 0;
        z-index: -1;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
        -ms-transition: all 0.5s;
    }

        .block-list-op-menu .list-item-menu.testing {
            display: block;
        }

.chose-item-menu-list {
    text-align: center;
    padding-top: 15px;
}

.block-list-op-menu .chose-item-menu-list li {
    display: inline-block;
    height: 36px;
    width: 70px;
    border: 1px solid #c7c7c7;
    text-align: center;
    padding-top: 6px;
    cursor: pointer;
    margin-right: 7px;
    transition: all 0.4s;
    -webkit-transition: all 0.4s;
    -moz-transition: all 0.4s;
    -ms-transition: all 0.4s;
}

.block-list-op-menu .chose-item-menu-list li {
    -webkit-transform: translate(-5px,-20px);
    -moz-transform: translate(-5px,-20px);
    -ms-transform: translate(-5px,-20px);
    transform: translate(-5px,-20px);
    opacity: 0;
}

    .block-list-op-menu .chose-item-menu-list li.chose-menu,
    .block-list-op-menu .chose-item-menu-list li:hover {
        background-color: #555;
        -webkit-transform: translate(0,-4px) !important;
        -moz-transform: translate(0,-4px) !important;
        -ms-transform: translate(0,-4px) !important;
        transform: translate(0,-4px) !important;
    }

