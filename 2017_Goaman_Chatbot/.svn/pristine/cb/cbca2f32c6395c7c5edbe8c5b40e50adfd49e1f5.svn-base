/*notofication*/
.alert.alert-success {
    position: fixed;
    top: 75px;
    left: 50%;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
    -o-transform: translateX(-50%);
    background: rgba(223,240,216,0.95);
    z-index: 9999;
    -webkit-animation: mymove 0.3s;
    animation: mymove 0.3s;
}
/* Chrome, Safari, Opera */
@-webkit-keyframes mymove {
    0% {
        top: -60px;
    }

    100% {
        top: 75px;
    }
}

/* Standard syntax */
@keyframes mymove {
    0% {
        top: -60px;
    }

    100% {
        top: 75px;
    }
}
