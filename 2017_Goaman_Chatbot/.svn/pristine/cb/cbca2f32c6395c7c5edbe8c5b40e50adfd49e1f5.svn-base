.type-menu-4-op li {
    display: inline;
    margin-right: 13px;
}

.type-menu-5-op li {
    display: inline-block;
    margin-right: 13px;
}

.type-menu-5-op a.menu__link {
    position: relative;
    color: #fff !important;
}

    .type-menu-5-op a.menu__link:hover {
        color: #e1b55a !important;
    }

.type-menu-5-op .menu__link:after {
    content: '';
    position: absolute;
    width: calc(100% - 30px);
    height: 2px;
    background-color: #e1b55a;
    bottom: 5px;
    left: 15px;
    opacity: 0;
    -webkit-transform: translateY(8px);
    -moz-transform: translateY(8px);
    -ms-transform: translateY(8px);
    transform: translateY(8px);
    transition: all 0.4s;
}

.type-menu-5-op .menu__link:hover:after,
.type-menu-5-op .menu__link.menu__item--current:after {
    -webkit-transform: translateY(0);
    -moz-transform: translateY(0);
    -ms-transform: translateY(0);
    transform: translateY(0);
    opacity: 1;
    background-color: #e1b55a;
}

.navi-type-3 .menu-des li:last-child i {
    position: absolute;
    bottom: -8px;
    width: 7px;
    height: 7px;
