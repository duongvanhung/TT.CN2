::-webkit-scrollbar {
    width: 10px;
    outline: solid black;
}

::-webkit-scrollbar-button {
    display: none;
}

::-webkit-scrollbar-track {
    border-style: solid;
    border-width: 2px;
    border-color: black;
}

::-webkit-scrollbar-track-piece {
    background-color: #343648;
    width: 10px;
}

::-webkit-scrollbar-thumb {
    background-color: #535673;
    border-style: solid;
    border-width: 8px 3px 8px 3px;
    border-color: #343648;
    width: 4px;
}
